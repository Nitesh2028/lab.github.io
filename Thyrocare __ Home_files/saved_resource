    <script>
        $(document).ready(function() {
            $("body").not("#searchedListBox *").click(function () {
                var searchedBox = $("#searchedListBox");
                var searchedBoxList = new $("#searchedListBox").find("ul");
                searchedBoxList.html("");
                searchedBox.hide();
            });

            $("#SignOut").click(function () {
                localStorage.clear();
            });

            //$('.glyphicon-search').click(function () {
            //    performSearch();
            //});
        });

        if (localStorage.getItem(LOCALSTORAGE.MstTests) != null) {
            var data = JSON.parse(localStorage.getItem(LOCALSTORAGE.MstTests));
            var dataList = data.value;
            var profileData = dataList.PROFILE;
            var testsData = dataList.TESTS;
            var offerData = dataList.OFFER;
            var dataForSearch = [];

            for (var i = 0; i < offerData.length; i++) {
                dataForSearch.push({
                    heading: 'Offer',
                    title: offerData[i].name
                });
            }

            for (var i = 0; i < profileData.length; i++) {
                dataForSearch.push({
                    heading: 'Profile',
                    title: profileData[i].name
                });
            }

            for (var t = 0; t < testsData.length; t++) {
                dataForSearch.push({
                    heading: 'Test',
                    title: testsData[t].name
                });
            }
        }

        function callSearchOnType() {
            var searchedBox = $("#searchedListBox");
            var searchInputvalue = document.querySelector("#search").value.toLowerCase();
            var SearchedResult = [];
            if (searchInputvalue.length > 1) {
                SearchedResult = dataForSearch.filter(function(data) {
                    var sData = data.title.toLowerCase();
                    return sData.indexOf(searchInputvalue) > -1
                });
                setSearchedList(SearchedResult);
            } else {
                searchedBox.hide();
            }
        }

        function callSearchFromItemClick(event) {
            var title = event.target.parentNode.querySelector(".title").innerHTML;
            searchData(title);
        }

        function setSearchedList(srdata) {
            var searchedBox = $("#searchedListBox");
            var searchedBoxList = $("#searchedListBox").find("ul");
            searchedBoxList.html("");
            searchedBox.show();
            for (var i = 0; i < srdata.length; i++) {
                searchedBoxList.append("<li onclick='callSearchFromItemClick(event); return false'><div class='item'><span class='heading'>" + srdata[i].heading + "</span><span class='title'>" + srdata[i].title + "</span></div></li>");
            }
        }

        function performSearch() {
            debugger
            var n = document.querySelector("#search").value;
            searchData(n);
        }

        function searchData(searchedValue) {
            window.location.href = "/Cart/Tests" + "?searchQuery=" + searchedValue;
        }

    function fncRedirectToCart() {
            window.location.href = "/Cart";
        }

