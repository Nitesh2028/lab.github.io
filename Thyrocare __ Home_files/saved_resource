    <script>
        $(document).ready(function () {
            var width = $(window).width();
            if (width < 768) {
                $("#cartPageLink").hide();
                $("#cartPageLinkMobile").show();

            }
            if (width > 768) {
                $("#cartPageLink").show();
                $("#cartPageLinkMobile").hide();
            }

            $(window).resize(function () {
                var width = $(window).width();
                if (width < 768) {
                    $("#cartPageLink").hide();
                    $("#cartPageLinkMobile").show();

                }
                if (width > 768) {
                    $("#cartPageLink").show();
                    $("#cartPageLinkMobile").hide();
                }
            });

        });
        $(window).bind('mousewheel', function (event) {
            if (event.originalEvent.wheelDelta >= 0) {
                $('#onscrollrm').show();
                $('#header').css("height", "auto");
            }
            else {
                $('#onscrollrm').hide();
                $('#header').css("height", "65px");
            }
        });
        var lastScrollTop = 0;
        $(window).on('scroll', function () {
            st = $(this).scrollTop();
            if (st < lastScrollTop) {
                $('#onscrollrm').show();
                $('#header').css("height", "auto");
            }
            else {
                $('#onscrollrm').hide();
                $('#header').css("height", "65px");
            }
            lastScrollTop = st;
        });

        function emptySearch() {

            var search = $('#search').val().length;
            if (search == 0) {
                window.location.href = "/Cart/Tests";
            }
        }
    </script>
